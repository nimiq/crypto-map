{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*"
  ],
  "compilerOptions": {
    "lib": [
      "DOM",
      "ES2017"
    ],
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "grecaptcha"
    ],
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ],
}
