{
  "name": "nimiq-map",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "yarn i18n:extract && yarn type-check && yarn build-only",
    "preview": "yarn i18n:extract && vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-pattern dist/ --ignore-pattern '*.config.js'",
    "story:dev": "yarn i18n:extract && histoire dev",
    "story:build": "yarn i18n:extract && histoire build",
    "story:preview": "yarn i18n:extract && histoire preview",
    "i18n:extract": "node ./node_modules/webpack-i18n-tools/index.js ./src/i18n/en.po",
    "i18n:pull": "tx pull -l de,es,fr,nl,tr,uk --force --minimum-perc 50",
    "i18n:push": "tx push --source",
    "i18n:sync": "yarn i18n:extract && yarn i18n:push; yarn i18n:pull"
  },
  "dependencies": {
    "@fontsource/mulish": "^4.5.7",
    "@googlemaps/js-api-loader": "^1.14.3",
    "@googlemaps/markerclusterer": "^2.0.14",
    "@headlessui/vue": "^1.7.2",
    "@nimiq/utils": "^0.9.1",
    "@nimiq/vue3-components": "github:nimiq/vue3-components#build/full",
    "@vueuse/components": "^10.2.1",
    "@vueuse/core": "^10.2.1",
    "@vueuse/math": "^10.2.1",
    "@vueuse/router": "^10.2.1",
    "pinia": "^2.0.16",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3",
    "vue3-google-map": "^0.15.0"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.16.1",
    "@types/google.maps": "^3.50.5",
    "@types/grecaptcha": "^3.0.4",
    "@types/node": "^16.11.45",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "google-maps": "^4.3.3",
    "histoire": "^0.16.2",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.3",
    "tailwindcss-nimiq-theme": "^0.2.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.1",
    "vite-plugin-rewrite-all": "^1.0.0",
    "vue-tsc": "^1.0.10",
    "webpack-i18n-tools": "https://github.com/nimiq/webpack-i18n-tools"
  }
}
